<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:gravity="center"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn"
        android:layout_width="match_parent"
        android:text="getFlutterVersion-不带入参-返回String"
        android:layout_height="48dp"/>

    <Button
        android:id="@+id/btn_map"
        android:layout_width="match_parent"
        android:text="callFlutterNoReturn-带入参-返回Map"
        android:layout_height="48dp"/>

    <Button
        android:id="@+id/btn_noreturn"
        android:layout_width="match_parent"
        android:text="callFlutterNoReturn-带入参"
        android:layout_height="48dp"/>



    <Button
        android:id="@+id/btn_notImplemented"
        android:layout_width="match_parent"
        android:text="method notImplemented"
        android:layout_height="48dp"/>

    <Button
        android:id="@+id/btn_testError"
        android:layout_width="match_parent"
        android:text="testError"
        android:layout_height="48dp"/>

    <Button
        android:id="@+id/btn_testError2"
        android:layout_width="match_parent"
        android:text="testError2"
        android:layout_height="48dp"/>

</LinearLayout>